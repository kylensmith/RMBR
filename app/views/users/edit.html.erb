<h1>User#edit</h1>
<p>Find me in app/views/user/new.html.erb</p>

<%= form_for @user do |f| %>   
			<%=f.label :First_Name %>   
			<%=f.text_field :fname %> 
			<br><br>
			<%=f.label :Last_name %>   
			<%=f.text_field :lname %>  
			<br><br>
			<%=f.label :email %>   
			<%=f.email_field :email %>  
			<br><br>
			<%=f.label :password %>   
			<%=f.password_field :password %>  
		
			<div class="button"><%=f.submit :update %></div>
			<% end %>

			