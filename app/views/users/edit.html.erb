<h1>User#edit</h1>
<p>Find me in app/views/user/new.html.erb</p>

<%= form_for @user do |f| %>   
	<%=f.label :email %>   
	<%=f.text_field :email %> 
	<br><br>
	<%=f.label :Password %>   
	<%=f.password_field :password %>
	<br><br>
	<%=f.label :birthday %>   
	<%=f.date_field :birthday %>  
	<br><br>  
	<div class="button"><%=f.submit :update %>
	<% end %>

