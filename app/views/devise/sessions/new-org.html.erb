<div class="grid-stack grid-stack-static grid-stack-6 top" style = "height:110px; margin-bottom:10px; z-index: 9999; background-color: rgba(255,255,255, 0.9);" >

    <div class="grid-stack-item" 
        data-gs-x="0" data-gs-y="0" 
        data-gs-width="1" data-gs-height="1" id="default">
            <div class="grid-stack-item-content"></div>
    </div>
    <div class="grid-stack-item" 
        data-gs-x="1" data-gs-y="0" 
        data-gs-width="4" data-gs-height="1" id="default">
            <div class="grid-stack-item-content">
                <div class="lg_top" style = "background-image: url(<%=asset_path 'logo.svg' %>)"></div>
            </div>
    </div>
    <div class="grid-stack-item" 
        data-gs-x="6" data-gs-y="0" 
        data-gs-width="1" data-gs-height="1" id="default">
            <div class="grid-stack-item-content"></div>
    </div>

</div>
<div class="grid-stack grid-stack-static grid-stack-6 main" style = "height:340px" >
    <div class="grid-stack-item"
        data-gs-x="0" data-gs-y="0" 
        data-gs-width="3" data-gs-height="2">
            <div class="grid-stack-item-content" style = "background-image: url(<%=asset_path 'phototest.jpg' %>)"><div class="main_elocdate">event location<br>event date</div><div class="main_etitle">event title here</div><div class="clear"></div></div>
    </div>
    <div class="grid-stack-item" 
        data-gs-x="3" data-gs-y="0" 
        data-gs-width="2" data-gs-height="1">
        <div class="grid-stack-item-content">
          <h2>Log in testing</h2>

          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="field">
            <%= f.label :email %><br />
            <%= f.email_field :email, autofocus: true %>
          </div>

          <div class="field">
            <%= f.label :password %><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>

          <% if devise_mapping.rememberable? -%>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          </div>
          <% end -%>

          <div class="actions">
            <%= f.submit "Log in" %>
          </div>
          <% end %>

          <%= render "devise/shared/links" %>
        </div>
      </div>
    <div class="grid-stack-item" 
        data-gs-x="5" data-gs-y="0" 
        data-gs-width="1" data-gs-height="1" id="default">
            <div class="grid-stack-item-content"></div>
    </div>
    <div class="grid-stack-item" 
        data-gs-x="3" data-gs-y="1" 
        data-gs-width="1" data-gs-height="1">
            <div class="grid-stack-item-content"></div>
    </div>
    <div class="grid-stack-item" 
        data-gs-x="4" data-gs-y="1" 
        data-gs-width="2" data-gs-height="1">
            <div class="grid-stack-item-content">
                <div class="lt_home" style = "background-image: url(<%=asset_path 'type_blue_ol.svg' %>)"></div>
            </div>
    </div>
</div>


